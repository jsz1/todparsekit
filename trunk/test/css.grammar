hexcolor     = '#' Num;
expr         = hexcolor | QuotedString | 'bold' | 'normal' | 'italic';
property     = 'color' | 'background-color' | 'font-weight' | 'font-style' | 'font-family' | 'font-size';
decl         = property ':' expr;
semiDecl     = ';' decl?;
actualRule   = decl semiDecl*;
rulesContent = Empty | actualRule;
selector     = LowercaseWord;
rules        = selector '{' rulesContent '}';
start        = rules*;